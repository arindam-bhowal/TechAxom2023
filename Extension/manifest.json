{
  "name": "My Extension",
  "version": "1.0",
  "manifest_version": 2,
  "permissions": [
    "http://192.168.247.28:8002/",
    "tabs",
    "https://*.youtube.com/*",
    "webNavigation",
		"storage"
  ],
  "browser_action": {
    "default_title": "My Chrome Extension",
    "default_popup": "popup.html"
  },
  "web_accessible_resources": [
    "assets/bookmark.png",
    "assets/play.png",
    "assets/delete.png",
    "assets/save.png"
  ],
  "background": {
    "scripts": ["background.js"],
    "persistent": false
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content.js"]
    }
  ]
}
